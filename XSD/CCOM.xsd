	<xs:complexType name="Organization">
		<xs:complexContent>
			<xs:extension base="Agent">
				<xs:sequence>
					<xs:element name="RegisteredSite" type="Site" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Sites that are registered to an Organization. Note that this is different to Agent.RegistrationSite, which indicates which Site registers an Organization.</xs:documentation>
