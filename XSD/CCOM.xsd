	<!--  *****************************  -->
	<!--  15 - Signal Processing Setups  -->
	<!--  *****************************  -->

	<xs:complexType name="AverageSynchType">
		<xs:annotation>
			<xs:documentation>Different kinds of signal processing averaging synchronization methods.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="BaseType"/>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="AverageType">
		<xs:annotation>
			<xs:documentation>Different kinds of signal processing averaging methods.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="BaseType"/>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="AverageWeightType">
		<xs:annotation>
			<xs:documentation>Different kinds of signal processing average weighting methods.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="BaseType"/>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="CPBSetup">
		<xs:annotation>
			<xs:documentation>Collection setup parameters for signal-processed real linear Constant Percentage Band data.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="SignalProcessSetup">
				<xs:sequence>
					<xs:element name="PostScalingType" type="PostScalingType" minOccurs="1" maxOccurs="1"/>
					<xs:element name="NumberOfBands" type="cct:NumericType" minOccurs="1" maxOccurs="1"/>
					<xs:element name="Center" type="Measure" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>First band's center frequency (e.g. in Hertz).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="BandWidth" type="Measure" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Valid unit of measure is either in Percent or Octave.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AverageType" type="AverageType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="AverageWeightType" type="AverageWeightType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="AverageSynchType" type="AverageSynchType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="AverageOverlap" type="Percentage" minOccurs="0" maxOccurs="1"/>
					<xs:element name="NumberOfAverages" type="cct:NumericType" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="FFTSetup">
		<xs:annotation>
			<xs:documentation>Collection setup parameters for signal-processed real/complex linear frequency/order FFT spectrum data.
Note: The bin width = (Maximum – Minimum) / ResolutionLines. The frequency/order associated with the first line of the spectrum = Minimum + the bin width.
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="SignalProcessSetup">
				<xs:sequence>
					<xs:element name="PostScalingType" type="PostScalingType" minOccurs="1" maxOccurs="1"/>
					<xs:element name="Minimum" type="Measure" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Valid unit of measure are either in Hertz or Orders.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Maximum" type="Measure" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Valid unit of measure are either in Hertz or Orders.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ResolutionLines" type="cct:NumericType" minOccurs="1" maxOccurs="1"/>
					<xs:element name="WindowType" type="WindowType" minOccurs="1" maxOccurs="1"/>
					<xs:element name="AverageType" type="AverageType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="AverageWeightType" type="AverageWeightType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="AverageSynchType" type="AverageSynchType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="AverageOverlap" type="Percentage" minOccurs="0" maxOccurs="1"/>
					<xs:element name="NumberOfAverages" type="cct:NumericType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="IsComplex" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="OrderedMeasLocationWithUOMItemSignalProcessSetup">
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:element name="OrderedMeasLocationWithUOMItem" type="OrderedMeasLocationWithUOMItem" minOccurs="0" maxOccurs="1"/>
					<xs:element name="SignalProcessSetup" type="SignalProcessSetup" minOccurs="1" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="PostScalingType">
		<xs:annotation>
			<xs:documentation>Different kinds of post-processed signal processing scaling mode methods.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="BaseType"/>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="SignalProcessBlock">
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:group ref="Nameable"/>
					<xs:element name="Type" type="SignalProcessBlockType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="StreamLink" type="SignalProcessBlockInStream" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="SignalProcessBlockType">
		<xs:complexContent>
			<xs:extension base="BaseType"/>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="SignalProcessBlockInStream">
		<xs:annotation>
			<xs:documentation>The processing sequence which data passed through.
Note: Care must be taken not to change the sequence of blocks in a stream if measurements have been stored for the stream. Normally, a new stream should be defined instead of updating an existing stream.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:element name="Stream" type="SignalProcessStream" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Block" type="SignalProcessBlock" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Order" type="cct:NumericType" minOccurs="1" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="SignalProcessSetup" abstract="true">
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:group ref="Nameable"/>
					<xs:element name="Stream" type="SignalProcessStream" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="SignalProcessStream">
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:group ref="Nameable"/>
					<xs:element name="BlockLink" type="SignalProcessBlockInStream" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="SignalProcessStreamType">
		<xs:complexContent>
			<xs:extension base="BaseType"/>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="SourceDetectorType">
		<xs:annotation>
			<xs:documentation>Different kinds of source signal detector types.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="BaseType"/>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="TimeWaveformSetup">
		<xs:annotation>
			<xs:documentation>Collection setup parameters for signal-processed real/complex linear time waveform data.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="SignalProcessSetup">
				<xs:sequence>
					<xs:element name="StartOffset" type="Measure" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Valid unit of measure is either in Seconds or Revolutions Per Minute.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Maximum" type="Measure" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Valid unit of measure is either in Seconds or Revolutions Per Minute.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="NumberOfSamples" type="cct:NumericType" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Delta offset start time in seconds or offset number of revolutions from the Start time when the sample started.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="WindowType" type="WindowType" minOccurs="1" maxOccurs="1"/>
					<xs:element name="AverageType" type="AverageType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="AverageWeightType" type="AverageWeightType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="AverageSynchType" type="AverageSynchType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="AverageOverlap" type="Percentage" minOccurs="0" maxOccurs="1"/>
					<xs:element name="NumberOfAverages" type="cct:NumericType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="IsComplex" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="VectorSetup">
		<xs:annotation>
			<xs:documentation>Collection setup parameters for signal-processed frequency/order single amplitude/vector data.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="SignalProcessSetup">
				<xs:sequence>
					<xs:element name="PostScalingType" type="PostScalingType" minOccurs="1" maxOccurs="1"/>
					<xs:element name="Minimum" type="Measure" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Valid unit of measure are either in Hertz or Orders.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Maximum" type="Measure" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Valid unit of measure are either in Hertz or Orders.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SourceDetectorType" type="SourceDetectorType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>If the source signal detector type is "FFT Power RMS", a VectorMeasurement Amplitude is calculated from the spectra by taking the square root of the sum of all amplitude values squared which is then divided by the window factor.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="WindowType">
		<xs:annotation>
			<xs:documentation>Different kinds of FFT and time waveform windows.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="BaseType">
				<xs:sequence>
					<xs:element name="PFMultiplier" type="cct:NumericType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Power factor multiplier for the window.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>


	<!--  ***************  -->
	<!--  16 - Algorithms  -->
	<!--  ***************  -->

	<xs:complexType name="Algorithm">
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:group ref="Nameable"/>
					<xs:element name="AlgorithmProcessType" type="AlgorithmProcessType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="IsTemplate" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Template" type="Algorithm" minOccurs="0" maxOccurs="1"/>
					<xs:element name="ProcessingURI" type="xs:anyURI" minOccurs="0" maxOccurs="1"/>
					<xs:element name="ProcessingDescription" type="ValueContent" minOccurs="0" maxOccurs="1"/>
					<xs:element name="PortIO" type="PortIO" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="EventType" type="EventType" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="AlgorithmProcessType">
		<xs:complexContent>
			<xs:extension base="BaseType"/>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="OSACBMDataType">
		<xs:complexContent>
			<xs:extension base="BaseType"/>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="OSACBMFunctionBlockType">
		<xs:complexContent>
			<xs:extension base="BaseType"/>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="Port">
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:element name="OSACBMFunctionBlockType" type="OSACBMFunctionBlockType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="OSACBMDataType" type="OSACBMDataType" minOccurs="0" maxOccurs="1"/>
					<xs:choice>
						<xs:element name="MeasurementLocationType" type="MeasurementLocationType" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>For use when a specific MeasurementLocation is unknown (e.g. for use in an Algorithm template).</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="MeasurementLocation" type="MeasurementLocation" minOccurs="0" maxOccurs="1"/>
						<xs:element name="AgentRoleType" type="AgentRoleType" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>For use when a specific Agent is unknown (e.g. for use in an Algorithm template).</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Agent" type="Agent" minOccurs="0" maxOccurs="1"/>
					</xs:choice>
					<xs:element name="HypotheticalEvent" type="HypotheticalEvent" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="ActualEvent" type="ActualEvent" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Note: This is considered the "inverse" of the Event source association.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="PortIO">
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:group ref="Nameable"/>
					<xs:element name="Algorithm" type="Algorithm" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Port" type="Port" minOccurs="1" maxOccurs="1"/>
					<xs:element name="Order" type="cct:NumericType" minOccurs="1" maxOccurs="1"/>
					<xs:element name="Direction" minOccurs="1" maxOccurs="1">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="Input"/>
								<xs:enumeration value="Output"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>


	<!--  ******************  -->
	<!--  17 - State Regions  -->
	<!--  ******************  -->

	<xs:complexType name="MeasurementLocationRegion">
		<xs:complexContent>
			<xs:extension base="Region">
				<xs:sequence>
					<xs:element name="MeasurementLocation" type="MeasurementLocation" minOccurs="1" maxOccurs="1"/>
					<xs:element name="MeasurementLocationType" type="MeasurementLocationType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Used to specify that the region only applies for a particular category of measurements that have this matching measurement location type.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CalculationType" type="CalculationType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Used to specify that the region only applies for a particular category of measurements that have this matching calculated data method.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="Region" abstract="true">
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:group ref="Nameable"/>
					<xs:element name="Type" type="RegionType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Order" type="cct:NumericType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Minimum" type="Measure" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Minimum value where region begins. If not present, then value is "-infinity".</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Maximum" type="Measure" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Minimum value where region ends. If not present, then value is "+infinity".</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="IsMinimumInclusive" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
					<xs:element name="IsMaximumInclusive" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
					<xs:element name="MinimumHysteresis" type="Measure" minOccurs="0" maxOccurs="1"/>
					<xs:element name="MaximumHysteresis" type="Measure" minOccurs="0" maxOccurs="1"/>
					<xs:element name="BandDelay" type="Measure" minOccurs="0" maxOccurs="1"/>
					<xs:element name="IncreasedAbnormalThreshold" minOccurs="0" maxOccurs="1">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="Minimum"/>
								<xs:enumeration value="Maximum"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="RegionType">
		<xs:complexContent>
			<xs:extension base="BaseType">
				<xs:sequence>
					<xs:element name="SeverityLevelType" type="SeverityLevelType" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="RegionValidDuringEventType">
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:element name="Region" type="Region" minOccurs="1" maxOccurs="1"/>
					<xs:element name="EventType" type="EventType" minOccurs="1" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="SeverityLevelType">
		<xs:complexContent>
			<xs:extension base="BaseType">
				<xs:sequence>
					<xs:element name="SeverityScale" type="cct:NumericType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Severity on a 0 to 100 scale where 100 is the maximum severity.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="TestComponentRegion">
		<xs:complexContent>
			<xs:extension base="Region">
				<xs:sequence>
					<xs:element name="TestComponent" type="TestComponent" minOccurs="1" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="TestComponentTypeRegion">
		<xs:complexContent>
			<xs:extension base="MeasurementLocationRegion">
				<xs:sequence>
					<xs:element name="TestType" type="TestType" minOccurs="1" maxOccurs="1"/>
					<xs:element name="TestComponentType" type="TestComponentType" minOccurs="1" maxOccurs="1"/>
					<xs:element name="MaterialItem" type="MaterialItem" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="VectorRegion">
		<xs:complexContent>
			<xs:extension base="MeasurementLocationRegion">
				<xs:sequence>
					<xs:element name="VectorSetup" type="VectorSetup" minOccurs="1" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>


	<!--  *****************  -->
	<!--  18 - Measurements  -->
	<!--  *****************  -->

	<xs:complexType name="CPBMeasurement">
		<xs:annotation>
			<xs:documentation>A signal-processed real linear Constant Percentage Band spectrum.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Measurement">
				<xs:sequence>
					<xs:element name="CPBSetup" type="CPBSetup" minOccurs="0" maxOccurs="1"/>
					<xs:element name="PostScalingType" type="PostScalingType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="RotationalSpeed" type="Measure" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Data" type="BinaryData" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>A variable-length binary field, containing 4-byte IEEE float real values for each band in the spectra.
The IEEE format is:
  -&gt;  Low-Order Byte First
	bits 0-22  -&gt; Mantissa
	bits 23-30 -&gt; Exponent
	bit 31     -&gt; Sign</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="DataQualityType">
		<xs:annotation>
			<xs:documentation>Various quality states with which data can be classified.
MIMOSA Reference Data entries come from the ISO 9000-2 status guidelines.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="BaseType"/>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="FFTMeasurement">
		<xs:annotation>
			<xs:documentation>A signal-processed real or complex FFT measurement that is related to frequency or order ranges.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Measurement">
				<xs:sequence>
					<xs:element name="FFTSetup" type="FFTSetup" minOccurs="0" maxOccurs="1"/>
					<xs:element name="PostScalingType" type="PostScalingType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="RotationalSpeed" type="Measure" minOccurs="0" maxOccurs="1"/>
					<xs:element name="IsComplex" type="xs:boolean" minOccurs="1" maxOccurs="1"/>
					<xs:element name="Data" type="BinaryData" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>A variable-length binary field, containing 4-byte IEEE float real values for each line of resolution in the spectra. Imaginary double values in 4-byte IEEE format follow the set of real values.
The IEEE format is:
  -&gt;  Low-Order Byte First
	bits 0-22  -&gt; Mantissa
	bits 23-30 -&gt; Exponent
	bit 31     -&gt; Sign</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="MeasurementGrid">
		<xs:annotation>
			<xs:documentation>2-D linear rectangular grid of numeric data associated with binary measurement data. Used for radiological data in thermography, thickness data in ultrasonic testing, etc.
Note: The Data field must be of type BinaryData. Grid data starts with coordinates (DataOrigin/X, DataOrigin/Y) and progressing vertically downward (increment Y) until DataEnd/Y is exceeded, whereupon Y is then set to DataOrigin/Y and X is incremented until (DataEnd/X, DataEnd/Y) is reached.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:element name="Measurement" type="Measurement" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Resolution" type="Coordinate" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Total dimensions of the rectangular grid related to the binary data (grid resolution).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DataOrigin" type="Coordinate" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>X, Y coordinates of the origin of the 0-based rectangular grid for the "data rectangle" (top left-most corner).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DataEnd" type="Coordinate" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>X, Y coordinates of the end of the 0-based rectangular grid for the "data rectangle" (lower right-most corner).</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="MeasurementAssociation">
		<xs:annotation>
			<xs:documentation>Measurements that are related to one another. This is used in vibration to associate a vibration measurement to its corresponding tachometer signal measurement and for multi-channel data that are related to each other.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:element name="Measurement" type="Measurement" minOccurs="1" maxOccurs="1"/>
					<xs:element name="RelatedMeasurement" type="Measurement" minOccurs="1" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="Measurement" abstract="true">
		<xs:annotation>
			<xs:documentation>A record of a measurement at a Measurement Location at an associated time.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:group ref="Nameable"/>
					<xs:element name="Order" type="cct:NumericType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Start" type="UTCDateTime" minOccurs="0" maxOccurs="1"/>
					<xs:element name="End" type="UTCDateTime" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Recorded" type="UTCDateTime" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>When the measurement was stored/captured on the original source system or platform.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CalculationParameter" type="cct:NumericType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Parameter "x" used for the calculation.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Confidence" type="Percentage" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Confidence in measurement accuracy where 0 is no confidence and 100 is complete confidence.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MeasurementLocation" type="MeasurementLocation" minOccurs="0" maxOccurs="1"/>
					<xs:element name="MeasurementSource" type="MeasurementSource" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Transducer" type="Transducer" minOccurs="0" maxOccurs="1"/>
					<xs:choice>
						<xs:annotation>
							<xs:documentation>Note: The Asset and Segment association allows a monitoring system to log the current installed serialized asset (if a segment-based measurement location) or the segment where the serialized asset is located (if an asset-based measurement location) at the time of measurement.</xs:documentation>
						</xs:annotation>
						<xs:element name="Asset" type="Asset" minOccurs="0" maxOccurs="1"/>
						<xs:element name="Segment" type="Segment" minOccurs="0" maxOccurs="1"/>
					</xs:choice>
					<xs:element name="DataQualityType" type="DataQualityType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="MeasurementLocationType" type="MeasurementLocationType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="CalculationType" type="CalculationType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Actual method that measurement was calculated from raw data.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MeasurementAssociation" type="MeasurementAssociation" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="TriggeredRegion" type="TriggeredRegionMeasurement" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="OffsetArrayMeasurement">
		<xs:annotation>
			<xs:documentation>Used for representing large amounts of numeric data with the same unit of measure that was captured intermittently over a period of time (at different offsets) relative to a single base reference time (Start field, which is an offset of 0).
Periodically collected time waveform numeric data (e.g. every second) should use Measurement.
Note: The Data field must be of type BinaryData and represent the data array associated with the offset data.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Measurement">
				<xs:sequence>
					<xs:element name="Count" type="cct:NumericType" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Total number of offset/value data pairs.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Offsets" type="BinaryData" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Variable-length array of time-related (e.g. millisecond, second) offsets from Start (time "0").</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Values" type="BinaryData" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Variable-length array of values corresponding to an offset.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="SingleDataMeasurement">
		<xs:complexContent>
			<xs:extension base="Measurement">
				<xs:sequence>
					<xs:element name="Data" type="ValueContent" minOccurs="1" maxOccurs="1"/>
					<xs:element name="MeasurementGrid" type="MeasurementGrid" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="TimeWaveformMeasurement">
		<xs:annotation>
			<xs:documentation>A signal-processed real or complex time waveform measurement. A complex time waveform is obtained when you have a decimated signal over a certain frequency range.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Measurement">
				<xs:sequence>
					<xs:element name="TimeWaveformSetup" type="TimeWaveformSetup" minOccurs="0" maxOccurs="1"/>
					<xs:element name="RotationalSpeed" type="Measure" minOccurs="0" maxOccurs="1"/>
					<xs:element name="IsComplex" type="xs:boolean" minOccurs="1" maxOccurs="1"/>
					<xs:element name="Data" type="BinaryData" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>A variable-length binary field, containing 4-byte IEEE float real values. Imaginary double values in 4-byte IEEE format follow the set of real values.
The IEEE format is:
  -&gt;  Low-Order Byte First
	bits 0-22  -&gt; Mantissa
	bits 23-30 -&gt; Exponent
	bit 31     -&gt; Sign</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="VectorMeasurement">
		<xs:annotation>
			<xs:documentation>A signal-processed single amplitude or single vector measurement that is related to frequency or order ranges.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Measurement">
				<xs:sequence>
					<xs:element name="VectorSetup" type="VectorSetup" minOccurs="0" maxOccurs="1"/>
					<xs:element name="PostScalingType" type="PostScalingType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="RotationalSpeed" type="Measure" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Amplitude" type="Measure" minOccurs="1" maxOccurs="1"/>
					<xs:element name="Direction" type="Measure" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Valid unit of measure is in Degrees.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>


	<!--  **********  -->
	<!--  19 - Tests  -->
	<!--  **********  -->

	<xs:complexType name="Sample">
		<xs:annotation>
			<xs:documentation>Note: ShortName is the alpha-numeric sample number.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Measurement">
				<xs:sequence>
					<xs:element name="Sampler" type="Agent" minOccurs="0" maxOccurs="1"/>
					<xs:element name="SampledMaterial" type="MaterialItem" minOccurs="0" maxOccurs="1"/>
					<xs:element name="ParentHierarchy" type="SampleHierarchy" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="ChildHierarchy" type="SampleHierarchy" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="SampleHierarchy">
		<xs:annotation>
			<xs:documentation>Note: ShortName is the alpha-numeric sample number.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:element name="Parent" type="Sample" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Child" type="Sample" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Order" type="cct:NumericType" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="Test">
		<xs:complexContent>
			<xs:extension base="Measurement">
				<xs:sequence>
					<xs:element name="Type" type="TestType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Tester" type="Agent" minOccurs="0" maxOccurs="1"/>
					<xs:element name="TestedMaterial" type="MaterialItem" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Sample" type="Sample" minOccurs="0" maxOccurs="1"/>
					<xs:element name="TestComponent" type="TestComponent" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="TestGroup" type="TestGroup" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="TestComponent">
		<xs:complexContent>
			<xs:extension base="Measurement">
				<xs:sequence>
					<xs:element name="Type" type="TestComponentType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Test" type="Test" minOccurs="0" maxOccurs="1"/>
					<xs:element name="TestGroupComponent" type="TestGroupComponent" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="TestComponentRegion" type="TestComponentRegion" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="TestComponentType">
		<xs:annotation>
			<xs:documentation>Different kinds of Test Components, which are the resulting categories of data from performing a Test.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="BaseType"/>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="TestGroupComponent">
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:element name="Order" type="cct:NumericType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="TestGroup" type="TestGroup" minOccurs="0" maxOccurs="1"/>
					<xs:element name="TestComponent" type="TestComponent" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="TestGroup">
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:group ref="Nameable"/>
					<xs:element name="Test" type="Test" minOccurs="0" maxOccurs="1"/>
					<xs:element name="TestGroupComponent" type="TestGroupComponent" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="TestType">
		<xs:complexContent>
			<xs:extension base="BaseType"/>
		</xs:complexContent>
	</xs:complexType>


	<!--  ****************************  -->
	<!--  20 - Triggered State Regions  -->
	<!--  ****************************  -->

	<xs:complexType name="GridTriggeredRegion">
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:element name="Resolution" type="Coordinate" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Total dimensions of the rectangular grid related to the binary image (grid resolution).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="HighlightType" type="HighlightType" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Highlight area type (arrow, rectangle, etc.).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DataOrigin" type="Coordinate" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>X, Y coordinates of the origin of the highlighted area or point in region. This data must be interpreted based on the highlight area type.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DataEnd" type="Coordinate" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>X, Y coordinates of the end of the highlighted area or point in region. This data must be interpreted based on the highlight area type.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TriggeredRegionMeasurement" type="TriggeredRegionMeasurement" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="HighlightType">
		<xs:annotation>
			<xs:documentation>Different kinds of highlight areas for drawing attention to certain locations; used for binary areas.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="BaseType"/>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="MeasurementLocationTriggeredRegion">
		<xs:complexContent>
			<xs:extension base="TimestampedEvent">
				<xs:sequence>
					<xs:element name="MeasurementLocation" type="MeasurementLocation" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Note: This could be considered the "inverse" of the Event source association. However, the more explicit association back to MeasurementLocation is used instead.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:choice>
						<xs:element name="Region" type="Region" minOccurs="1" maxOccurs="1"/>
						<xs:element name="RegionType" type="RegionType" minOccurs="1" maxOccurs="1"/>
					</xs:choice>
					<xs:element name="Acknowledged" type="UTCDateTime" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Acknowledger" type="Agent" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Agent that acknowledged the triggered region.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TriggeredRegionMeasurement" type="TriggeredRegionMeasurement" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="TriggeredRegionTestComponent" type="TriggeredRegionTestComponent" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="TriggeredRegionMeasurement">
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:element name="MeasurementLocationTriggeredRegion" type="MeasurementLocationTriggeredRegion" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Measurement" type="Measurement" minOccurs="0" maxOccurs="1"/>
					<xs:element name="GridTriggeredRegion" type="GridTriggeredRegion" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="TriggeredRegionTestComponent">
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:element name="MeasurementLocationTriggeredRegion" type="MeasurementLocationTriggeredRegion" minOccurs="0" maxOccurs="1"/>
					<xs:element name="TestComponent" type="TestComponent" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>


	<!--  **************************************  -->
	<!--  21 & 22 - Events & Event Relationships  -->
	<!--  **************************************  -->

	<xs:complexType name="ActualEvent">
		<xs:annotation>
			<xs:documentation>An actual logged event (alarm/operational change/abnormal condition) related to a segment or asset at a specific point in time or during a specific interval of time.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="TimestampedEvent">
				<xs:sequence>
					<xs:element name="SubstantiatedByMeasurement" type="SubstantiatedByMeasurement" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="AgentRoleWithEvent">
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:element name="AgentRole" type="AgentRole" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Event" type="Event" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="ChangePatternType">
		<xs:annotation>
			<xs:documentation>Different kinds of change patterns observed in problems.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="BaseType"/>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="Event" abstract="true">
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:group ref="Nameable"/>
					<xs:element name="Type" type="EventType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Entity" type="Entity" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Typically Events are associated with Assets, AssetTypes, Segments, SegmentTypes, Models, and Ports. Specific Entity subclasses define the "inverse" association to specific subclasses of Event.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CauseLink" type="EventLink" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="EffectLink" type="EventLink" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="AgentRoleWithEvent" type="AgentRoleWithEvent" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="FunctionAffectedByEvent" type="FunctionAffectedByEvent" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="RecommendationFromEvent" type="RecommendationFromEvent" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="EventLink">
		<xs:annotation>
			<xs:documentation>Cause and effect linkages between events in a causal network.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:element name="CauseEvent" type="Event" minOccurs="0" maxOccurs="1"/>
					<xs:element name="EffectEvent" type="Event" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Likelihood" type="Probability" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="EventType">
		<xs:annotation>
			<xs:documentation>Different kinds of events including operating events and failure events such as Logged Operating Events, Failure "Modes", Failure "Effects", Failure "Mechanisms", Defect Events Abnormal Situations, Alarm Events, etc.
An event is NOT work performed by an entity, but a log of the actual events which occurred at a service segment or asset over time.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="BaseType">
				<xs:sequence>
					<xs:element name="SolutionPackage" type="SolutionPackage" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="TimestampedEvent" abstract="true">
		<xs:annotation>
			<xs:documentation>If either a Start or End is provided, this indicates either a point-in-time event or that the other attribute is unknown.

If both a Start and End is provided, this indicates an activity with duration.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Event">
				<xs:sequence>
					<xs:element name="Start" type="UTCDateTime" minOccurs="0" maxOccurs="1"/>
					<xs:element name="End" type="UTCDateTime" minOccurs="0" maxOccurs="1"/>
					<xs:element name="StartPosition" type="GeoPosition" minOccurs="0" maxOccurs="1"/>
					<xs:element name="EndPosition" type="GeoPosition" minOccurs="0" maxOccurs="1"/>
					<xs:element name="RequestCause" type="Request" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="RequestEffect" type="Request" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="ActiveWorkCause" type="ActiveWork" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="ActiveWorkEffect" type="ActiveWork" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="CompletedWorkCause" type="CompletedWork" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="CompletedWorkEffect" type="CompletedWork" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="Evidence">
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:group ref="Nameable"/>
					<xs:choice>
						<xs:element name="ActualEvent" type="ActualEvent" minOccurs="0" maxOccurs="1"/>
						<xs:element name="Measurement" type="Measurement" minOccurs="0" maxOccurs="1"/>
					</xs:choice>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="HypotheticalEvent">
		<xs:annotation>
			<xs:documentation>Possible failure modes and effects from an engineering study (often FMECA and RCM analysis).</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Event">
				<xs:sequence>
					<xs:element name="SeverityLevelType" type="SeverityLevelType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="ProposedEventForHypotheticalEvent" type="ProposedEventForHypotheticalEvent" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="OrderedList" type="OrderedList" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Can be used to detail the ordered lists of Algorithm templates that can be used to detect a failure mode (HypotheticalEvent).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Document" type="Document" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="SolutionPackage" type="SolutionPackage" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="ProposedEvent">
		<xs:annotation>
			<xs:documentation>A proposed event that might have occurred in the past, present or future that explains events and measurement events, including diagnostic/prognostic event outputs.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="TimestampedEvent">
				<xs:sequence>
					<xs:element name="SeverityLevelType" type="SeverityLevelType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Likelihood" type="Probability" minOccurs="0" maxOccurs="1"/>
					<xs:element name="ChangePatternType" type="ChangePatternType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Audit" type="Audit" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Evidence" type="Evidence" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="ProposedEventForHypotheticalEvent" type="ProposedEventForHypotheticalEvent" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="ProposedEventForHypotheticalEvent">
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:group ref="Nameable"/>
					<xs:element name="ProposedEvent" type="ProposedEvent" minOccurs="0" maxOccurs="1"/>
					<xs:element name="HypotheticalEvent" type="HypotheticalEvent" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="SubstantiatedByMeasurement">
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:group ref="Nameable"/>
					<xs:element name="Measurement" type="Measurement" minOccurs="1" maxOccurs="1"/>
					<xs:element name="ByAgent" type="Agent" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>


	<!--  ************************  -->
	<!--  23 - Geospatial Tracking  -->
	<!--  ************************  -->

	<xs:complexType name="GeoPosition">
		<xs:annotation>
			<xs:documentation>An identifier of a geographic place on the earth.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:group ref="Nameable"/>
					<xs:element name="GPSLocation" type="GPSLocation" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="GPSDatumType">
		<xs:complexContent>
			<xs:extension base="BaseType"/>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="GPSElevationType">
		<xs:complexContent>
			<xs:extension base="BaseType"/>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="GPSLocation">
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:element name="Order" type="cct:NumericType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Latitude" type="cct:NumericType" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>For 2D and 3D GPS position.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Longitude" type="cct:NumericType" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>For 2D and 3D GPS position.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Elevation" type="cct:NumericType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>For 3D GPS position.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="GPSDatumType" type="GPSDatumType" minOccurs="1" maxOccurs="1"/>
					<xs:element name="GPSPrecisionType" type="GPSPrecisionType" minOccurs="1" maxOccurs="1"/>
					<xs:element name="GPSElevationType" type="GPSElevationType" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="GPSPrecisionType">
		<xs:complexContent>
			<xs:extension base="BaseType"/>
		</xs:complexContent>
	</xs:complexType>


	<!--  *******************  -->
	<!--  24 - Ambiguity Sets  -->
	<!--  *******************  -->

	<xs:complexType name="AmbiguitySet" abstract="true">
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:group ref="Nameable"/>
					<xs:element name="Type" type="AmbiguitySetType" minOccurs="0" maxOccurs="1"/>
					<xs:choice>
						<xs:element name="Segment" type="Segment" minOccurs="0" maxOccurs="1"/>
						<xs:element name="Asset" type="Asset" minOccurs="0" maxOccurs="1"/>
					</xs:choice>
					<xs:element name="Connector" type="LogicalConnector" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="AmbiguitySetType">
		<xs:annotation>
			<xs:documentation>Different kinds of ambiguity set logical expression formats.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="BaseType"/>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="HypotheticalEventAmbiguitySet">
		<xs:complexContent>
			<xs:extension base="AmbiguitySet"/>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="HypotheticalEventLogicalConnector">
		<xs:complexContent>
			<xs:extension base="LogicalConnector">
				<xs:sequence>
					<xs:element name="LeafHypotheticalEvent" type="HypotheticalEvent" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="LogicalConnector" abstract="true">
		<xs:annotation>
			<xs:documentation>Allows logical expressions to be represented as a logic tree with hypothetical or proposed event leaf nodes at the bottom. Reverse polish notation of the logical expression is readly formed from this tree.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:element name="Type" type="LogicalConnectorType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Order" type="cct:NumericType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Likelihood" type="Probability" minOccurs="1" maxOccurs="1"/>
					<xs:element name="AmbiguitySet" type="AmbiguitySet" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Child" type="LogicalConnector" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="LogicalConnectorType">
		<xs:annotation>
			<xs:documentation>Different kinds of logical expression formats.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="BaseType"/>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="ProposedEventAmbiguitySet">
		<xs:complexContent>
			<xs:extension base="AmbiguitySet">
				<xs:sequence>
					<xs:element name="Start" type="UTCDateTime" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Estimated time when event started or will start.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="End" type="UTCDateTime" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Estimated time when event ended or will end.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PredefinedAmbiguitySet" type="HypotheticalEventAmbiguitySet" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Pointer to pre-defined hypothetical event ambiguity set, if not generating a new set.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SubstantiatingEvent" type="ActualEvent" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="ProposedEventLogicalConnector">
		<xs:complexContent>
			<xs:extension base="LogicalConnector">
				<xs:sequence>
					<xs:element name="LeafProposedEvent" type="ProposedEvent" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>


	<!--  **************************************  -->
	<!--  25 - Health and Prognostic Assessments  -->
	<!--  **************************************  -->

	<xs:complexType name="AgentRoleWithAssessment">
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:element name="AgentRole" type="AgentRole" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Assessment" type="Assessment" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="Assessment" abstract="true">
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:group ref="Nameable"/>
					<xs:element name="Assessed" type="UTCDateTime" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Timestamp when the assessment was performed by the agent.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AgentRoleWithAssessment" type="AgentRoleWithAssessment" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="Audit" type="Audit" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="Audit">
		<xs:complexContent>
			<xs:extension base="Assessment">
				<xs:sequence>
					<xs:element name="DataQualityType" type="DataQualityType" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="HealthAssessment">
		<xs:annotation>
			<xs:documentation>An assessment of a specific entity's health.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Assessment">
				<xs:sequence>
					<xs:element name="HealthLevelPrecise" type="cct:NumericType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Precise health level from a scale of 0 to 1 where 1 is perfect health.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Confidence" type="Percentage" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Confidence that the assessment is accurate.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="HealthLevelType" type="HealthLevelType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="ChangePatternType" type="ChangePatternType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="SubstantiatingEvent" type="TimestampedEvent" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="RecommendationFromHealthAssessment" type="RecommendationFromHealthAssessment" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="HealthLevelType">
		<xs:complexContent>
			<xs:extension base="BaseType">
				<xs:sequence>
					<xs:element name="HealthScale" type="cct:NumericType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Health on a 0 to 100 scale where 100 is the maximum health.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="RemainingLifeAssessment">
		<xs:annotation>
			<xs:documentation>An assessment of the remaining life of an entity.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Assessment">
				<xs:sequence>
					<xs:element name="Estimate" type="Measure" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Estimated remaining life.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Error" type="Measure" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Error estimate (± X duration).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Confidence" type="Percentage" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Confidence that the assessment is accurate.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>


	<!--  ********************  -->
	<!--  26 - Recommendations  -->
	<!--  ********************  -->

	<xs:complexType name="Recommendation">
		<xs:annotation>
			<xs:documentation>Used to capture recommendations resulting from an event or health assessment (e.g. as part of an advisory generation process), possibly triggering a request for work.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:group ref="Nameable"/>
					<xs:choice>
						<xs:element name="SegmentType" type="SegmentType" minOccurs="0" maxOccurs="1"/>
						<xs:element name="AssetType" type="AssetType" minOccurs="0" maxOccurs="1"/>
						<xs:element name="Segment" type="Segment" minOccurs="0" maxOccurs="1"/>
						<xs:element name="Model" type="Model" minOccurs="0" maxOccurs="1"/>
						<xs:element name="Asset" type="Asset" minOccurs="0" maxOccurs="1"/>
						<xs:element name="OrderedList" type="OrderedList" minOccurs="0" maxOccurs="1"/>
					</xs:choice>
					<xs:element name="PriorityLevelType" type="PriorityLevelType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="RecommendationFromEvent" type="RecommendationFromEvent" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="RecommendationFromHealthAssessment" type="RecommendationFromHealthAssessment" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="SubstantiatedByMeasurement" type="SubstantiatedByMeasurement" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="RequestForWork" type="RequestForWork" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="RecommendationWorkRequest" type="RecommendationWorkRequest" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="RecommendationWorkStep" type="RecommendationWorkStep" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="RecommendationFromEvent">
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:element name="Recommendation" type="Recommendation" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Event" type="Event" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="RecommendationFromHealthAssessment">
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:element name="Recommendation" type="Recommendation" minOccurs="0" maxOccurs="1"/>
					<xs:element name="HealthAssessment" type="HealthAssessment" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>


	<!--  **********************  -->
	<!--  27 - Solution Packages (Pre-planned Work Packages) -->
	<!--  *************************************************  -->

	<xs:complexType name="AgentRoleWithWork">
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:element name="AgentRole" type="AgentRole" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Work" type="Work" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="Cycle">
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:element name="RepeatInterval" type="Measure" minOccurs="1" maxOccurs="1"/>
					<xs:element name="MeasurementLocation" type="MeasurementLocation" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="PriorityLevelType">
		<xs:annotation>
			<xs:documentation>A categorization of priority. For example, High Priority, Low Priority.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="BaseType">
				<xs:sequence>
					<xs:element name="PriorityScale" type="cct:NumericType" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="SolutionPackage">
		<xs:annotation>
			<xs:documentation>A pre-defined set of work that can specify the exact required resources of materials, tools, documents and labor required to complete the job.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Work">
				<xs:sequence>
					<xs:element name="Type" type="SolutionPackageType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="ParentHierarchy" type="SolutionPackageHierarchy" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="ChildHierarchy" type="SolutionPackageHierarchy" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="SegmentType" type="SegmentType" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="AssetType" type="AssetType" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="Segment" type="Segment" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="Asset" type="Asset" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="OrderedList" type="OrderedList" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="Step" type="SolutionPackageStep" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="Cycle" type="Cycle" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="HypotheticalEvent" type="HypotheticalEvent" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="SolutionPackageHierarchy">
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:element name="Parent" type="SolutionPackage" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Child" type="SolutionPackage" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Order" type="cct:NumericType" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="SolutionPackageStep">
		<xs:complexContent>
			<xs:extension base="Work">
				<xs:sequence>
					<xs:element name="Order" type="cct:NumericType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="SolutionPackage" type="SolutionPackage" minOccurs="0" maxOccurs="1"/>
					<xs:choice>
						<xs:element name="SegmentType" type="SegmentType" minOccurs="0" maxOccurs="1"/>
						<xs:element name="AssetType" type="AssetType" minOccurs="0" maxOccurs="1"/>
						<xs:element name="Segment" type="Segment" minOccurs="0" maxOccurs="1"/>
						<xs:element name="Asset" type="Asset" minOccurs="0" maxOccurs="1"/>
						<xs:element name="OrderedList" type="OrderedList" minOccurs="0" maxOccurs="1"/>
					</xs:choice>
					<xs:element name="RequiredOrderedList" type="OrderedList" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Associated ordered lists (Bill of Material, Bill of Labor, etc.) needed by a Solution Package Step.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="WorkStep" type="WorkStep" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="SolutionPackageType">
		<xs:complexContent>
			<xs:extension base="BaseType"/>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="Work" abstract="true">
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:group ref="Nameable"/>
					<xs:element name="PriorityLevelType" type="PriorityLevelType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="WorkManagementType" type="WorkManagementType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="WorkTaskType" type="WorkTaskType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="AgentRoleWithWork" type="AgentRoleWithWork" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="WorkManagementType">
		<xs:complexContent>
			<xs:extension base="BaseType"/>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="WorkTaskType">
		<xs:complexContent>
			<xs:extension base="BaseType">
				<xs:sequence>
					<xs:element name="WorkManagementType" type="WorkManagementType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>This WorkTaskType is only applicable for the specified WorkManagementType.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>


	<!--  *************  -->
	<!--  28 - Requests  -->
	<!--  *************  -->

	<xs:complexType name="Request">
		<xs:annotation>
			<xs:documentation>Holds agents' requisitions for work, materials, tools, and/or labor.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:group ref="Nameable"/>
					<xs:element name="Type" type="RequestType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="FromAgent" type="Agent" minOccurs="0" maxOccurs="1"/>
					<xs:element name="ToAgent" type="Agent" minOccurs="0" maxOccurs="1"/>
					<xs:element name="PriorityLevelType" type="PriorityLevelType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="EngineeringStudyEntry" type="EngineeringStudyEntry" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="RequiredOrderedList" type="OrderedList" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Pre-work ordered list (e.g. Bill of Material, Bill of Labor) needed for a request.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EventCause" type="TimestampedEvent" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="EventEffect" type="TimestampedEvent" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="ParentRequest" type="RequestChild" minOccurs="0" maxOccurs="1"/>
					<xs:element name="ChildRequest" type="RequestChild" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
	  </xs:complexContent>
	</xs:complexType>

	<xs:complexType name="RequestType">
		<xs:annotation>
			<xs:documentation>This entity standardizes the kinds of requisitions that can be made by an agent. For example, a request kind could be created for Make and Model Match-up.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="BaseType"/>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="RequestChild">
		<xs:annotation>
			<xs:documentation>A Request may comprise a set of child Requests. If order is specified, it implies that the child Requests should be completed in the specified order.

The child Requests are considered contained in the parent for purposes of deletion semantics. For example, if the parent Request is soft-deleted, all child requests (that is both the RequestChild relationship and the Request itself) must be soft-deleted. 

Note: there is no constraint on the propagation of lifecycle status changes as valid propagation depends on the type of Request. That is, if a parent Request is marked complete it is not implied that all child Requests are complete.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:element name="Parent" type="Request" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Child" type="Request" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Order" type="cct:NumericType" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="RequestForInformation">
		<xs:annotation>
			<xs:documentation>
				A formal requisition for information about something of interest.
				For example, an RFI might be issued between an O/O and a supplier to obtain information regarding model specifications.
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Request">
				<xs:sequence>
					<xs:element name="RespondBy" type="UTCDateTime" minOccurs="0" maxOccurs="1"/>
					<xs:element name="ReferredEntity" type="Entity" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="RequestForWork">
		<xs:annotation>
			<xs:documentation>An requisition for an action to be taken to a work management system.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Request">
				<xs:sequence>
					<xs:choice>
						<xs:element name="Segment" type="Segment" minOccurs="0" maxOccurs="1"/>
						<xs:element name="Asset" type="Asset" minOccurs="0" maxOccurs="1"/>
						<xs:element name="OrderedList" type="OrderedList" minOccurs="0" maxOccurs="1"/>
					</xs:choice>
					<xs:element name="WorkManagementType" type="WorkManagementType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="WorkTaskType" type="WorkTaskType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="IsAutomaticallyApproved" type="xs:boolean" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Request automatic generation of work order.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="StartBefore" type="UTCDateTime" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Request for action to begin before this time.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EndBefore" type="UTCDateTime" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Request for action to end before this time.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="StartAfter" type="UTCDateTime" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Request for action to begin after this time.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EndAfter" type="UTCDateTime" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Request for action to end after this time.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="RepeatInterval" type="Measure" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Time interval to re-submit time-based actions.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SolutionPackage" type="SolutionPackage" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Recommendation" type="Recommendation" minOccurs="0" maxOccurs="1"/>
					<xs:element name="ActiveWork" type="ActiveWork" minOccurs="0" maxOccurs="2">
						<xs:annotation>
							<xs:documentation>Associated Work Request and/or Work Order.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>


	<!--  ********************  -->
	<!--  29 - Work Management  -->
	<!--  ********************  -->

	<xs:complexType name="ActiveWork" abstract="true">
		<xs:complexContent>
			<xs:extension base="CalendaredWork">
				<xs:sequence>
					<xs:element name="SolutionPackage" type="SolutionPackage" minOccurs="0" maxOccurs="1"/>
					<xs:element name="TimestampedEvent" type="TimestampedEvent" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="RequestForWork" type="RequestForWork" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="EventCause" type="TimestampedEvent" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="EventEffect" type="TimestampedEvent" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="CalendaredWork" abstract="true">
		<xs:complexContent>
			<xs:extension base="Work">
				<xs:sequence>
					<xs:element name="StartBefore" type="UTCDateTime" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Request for action to begin before this time.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EndBefore" type="UTCDateTime" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Request for action to end before this time.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="StartAfter" type="UTCDateTime" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Request for action to begin after this time.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EndAfter" type="UTCDateTime" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Request for action to end after this time.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ActualStart" type="UTCDateTime" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Actual start time.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ActualEnd" type="UTCDateTime" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Actual completion time.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:choice>
						<xs:element name="Segment" type="Segment" minOccurs="0" maxOccurs="1"/>
						<xs:element name="Asset" type="Asset" minOccurs="0" maxOccurs="1"/>
						<xs:element name="OrderedList" type="OrderedList" minOccurs="0" maxOccurs="1"/>
					</xs:choice>
					<xs:element name="EngineeringStudyEntry" type="EngineeringStudyEntry" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="WorkStatus" type="WorkStatus" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="CompletedWork" type="CompletedWork" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="RecommendationWorkRequest">
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:element name="Recommendation" type="Recommendation" minOccurs="0" maxOccurs="1"/>
					<xs:element name="WorkRequest" type="WorkRequest" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="RecommendationWorkStep">
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:element name="Recommendation" type="Recommendation" minOccurs="0" maxOccurs="1"/>
					<xs:element name="WorkStep" type="WorkStep" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="WorkStatus">
		<xs:annotation>
			<xs:documentation>Defines the status of the work at a given point in time.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Assessment">
				<xs:sequence>
					<xs:element name="Type" type="WorkStatusType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="CalendaredWork" type="CalendaredWork" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="WorkStatusType">
		<xs:complexContent>
			<xs:extension base="BaseType"/>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="WorkOrder">
		<xs:annotation>
			<xs:documentation>A collection of one or more Work Requests (optionally associated with Solution Packages) which are broken into steps, scheduled, and completed over time.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ActiveWork">
				<xs:sequence>
					<xs:element name="TriggeringCycle" type="Cycle" minOccurs="0" maxOccurs="1"/>
					<xs:element name="WorkRequest" type="WorkRequest" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="Step" type="WorkStep" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="ParentHierarchy" type="WorkOrderHierarchy" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="ChildHierarchy" type="WorkOrderHierarchy" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="StandingCycle" type="Cycle" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="WorkOrderHierarchy">
		<xs:annotation>
			<xs:documentation>Relationship of master parent work order to follow-on work orders.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:element name="Parent" type="WorkOrder" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Child" type="WorkOrder" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Order" type="cct:NumericType" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="WorkRequest">
		<xs:annotation>
			<xs:documentation>Created and tracked by a work management system, once a request for work has been transmitted and successfully received.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ActiveWork">
				<xs:sequence>
					<xs:element name="WorkOrder" type="WorkOrder" minOccurs="0" maxOccurs="1"/>
					<xs:element name="RecommendationWorkRequest" type="RecommendationWorkRequest" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="WorkStep">
		<xs:annotation>
			<xs:documentation>An individual step of a Solution Package, Work Request, or Work Order.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="CalendaredWork">
				<xs:sequence>
					<xs:element name="WorkOrder" type="WorkOrder" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Order" type="cct:NumericType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="SolutionPackageStep" type="SolutionPackageStep" minOccurs="0" maxOccurs="1"/>
					<xs:element name="RequiredOrderedList" type="OrderedList" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Associated ordered lists (Bill of Material, Bill of Labor, etc.) needed by a Work Step.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="RecommendationWorkStep" type="RecommendationWorkStep" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>


	<!--  *******************  -->
	<!--  30 - Completed Work  -->
	<!--  *******************  -->

	<xs:complexType name="CompletedWork">
		<xs:annotation>
			<xs:documentation>Notes the actual work performed.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="CalendaredWork">
				<xs:sequence>
					<xs:element name="UsedOrderedList" type="OrderedList" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="RemovedOrderedList" type="OrderedList" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="EventCause" type="TimestampedEvent" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="EventEffect" type="TimestampedEvent" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>


	<!--  **************  -->
	<!--  31 - Documents  -->
	<!--  **************  -->

	<xs:complexType name="Document">
		<xs:complexContent>
			<xs:extension base="InfoSource">
				<xs:sequence>
					<xs:element name="Language" type="xs:language" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>The primary language of the document.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Content" type="BinaryObject" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>The data content of the document.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="FileName" type="cct:TextType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Default name for the data in Content if it is extracted into a file.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EntityContent" type="EntityContent" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="DocumentForAsset" type="DocumentForAsset" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="DocumentForModel" type="DocumentForModel" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="DocumentForSegment" type="DocumentForSegment" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="DocumentForAsset">
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:element name="Document" type="Document" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Asset" type="Asset" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="DocumentForModel">
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:element name="Document" type="Document" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Model" type="Model" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="DocumentForSegment">
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:element name="Document" type="Document" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Segment" type="Segment" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="DocumentType">
		<xs:complexContent>
			<xs:extension base="InfoSourceType"/>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="EntityContent">
		<xs:annotation>
			<xs:documentation>Association class to describe structured content that can represented as CCOM data. For example, a Piping and Instrumentation Diagram (P&amp;ID) that contains tags represented as Segments and process and signal networks represented as SegmentMeshes.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:element name="Entity" type="Entity" minOccurs="1" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

</xs:schema>
