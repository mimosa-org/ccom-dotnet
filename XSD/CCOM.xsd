	<xs:complexType name="Asset">
		<xs:annotation>
			<xs:documentation>A tangible, instantiated, serialized object. An Asset may be an entire facility, an entire functioning platform (such as an CH-47 Tail Number XYZ helicopter), or a component piece of equipment, such as a specific instance of a bearing. An Asset can be tracked over time and installed in a Segment. It may then be removed from the Segment and re-installed in a different Segment.
Note: ShortName is the user identification tag which follows the nomenclature of the plant. This is normally an abbreviation or user-defined alpha-numeric code from which the plant derives meaning.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:group ref="Nameable"/>
					<xs:element name="Type" type="AssetType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="RegistrationSite" type="Site" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Manufacturer" type="Manufacturer" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>The manufacturer that "makes" the Asset.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Model" type="Model" minOccurs="0" maxOccurs="1"/>
					<xs:choice>
						<xs:element name="LifecycleStatus" type="LifecycleStatusHistory" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Past/present/future lifecycle status. 
This field is deprecated and has been replaced by the LifecycleStatusHistory element of Entity.
This maps to LifecycleStatusHistory type of the expanded model.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="LifecycleStatusType" type="LifecycleStatusType" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>Present lifecycle status.
This field is deprecated and has been replaced by the PresentLifecycleStatus element of Entity.
This maps to LifecycleStatusType type of the expanded model.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
					<xs:element name="AssetModelEvent" type="AssetModelEvent" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="SerialNumber" type="cct:IDType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="ParentComponent" type="AssetComponent" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="ChildComponent" type="AssetComponent" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="Criticality" type="Criticality" minOccurs="0" maxOccurs="1"/>
					<xs:element name="ReadinessType" type="ReadinessType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="PurchaseConditionType" type="PurchaseConditionType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="AssetSegmentEvent" type="AssetSegmentEvent" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="AssetOwnerEvent" type="AssetOwnerEvent" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="AgentRoleWithAsset" type="AgentRoleWithAsset" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="MeasurementLocation" type="MeasurementLocation" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="NetworkForAsset" type="NetworkForAsset" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="FromConnection" type="AssetConnection" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="ToConnection" type="AssetConnection" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="Function" type="Function" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="EngineeringStudyEntryForAsset" type="EngineeringStudyEntryForAsset" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="OrderedListForAsset" type="OrderedListForAsset" minOccurs="0" maxOccurs="unbounded"/>         
					<xs:element name="ActualEvent" type="ActualEvent" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Note: This collection will only include direct instances of ActualEvent and not its subclasses (i.e., AssetSegmentEvent, AssetModelEvent, and AssetOwnerEvent).

Note: This is considered the "inverse" of the Event source association.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="HypotheticalEvent" type="HypotheticalEvent" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Note: This is considered the "inverse" of the Event source association.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ProposedEvent" type="ProposedEvent" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Note: This is considered the "inverse" of the Event source association.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AmbiguitySet" type="AmbiguitySet" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="HealthAssessment" type="HealthAssessment" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="RemainingLifeAssessment" type="RemainingLifeAssessment" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="Recommendation" type="Recommendation" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="SolutionPackage" type="SolutionPackage" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="SolutionPackageStep" type="SolutionPackageStep" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="RequestForWork" type="RequestForWork" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="CalendaredWork" type="CalendaredWork" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="AssetComponent">
		<xs:annotation>
			<xs:documentation>A breakdown of assets into sub-asset components (child assets), to form serialized component part breakdown trees.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:element name="Parent" type="Asset" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Child" type="Asset" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Order" type="cct:NumericType" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="AssetType">
		<xs:complexContent>
			<xs:extension base="BaseType">
				<xs:sequence>
					<xs:element name="MaterialMasterItem" type="MaterialMasterItem" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="Function" type="Function" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="EngineeringStudyEntryForAssetType" type="EngineeringStudyEntryForAssetType" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="HypotheticalEvent" type="HypotheticalEvent" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Note: This is considered the "inverse" of the Event source association.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Recommendation" type="Recommendation" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="SolutionPackage" type="SolutionPackage" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="SolutionPackageStep" type="SolutionPackageStep" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="Criticality">
		<xs:sequence>
			<xs:element name="CriticalityScaleType" type="CriticalityScaleType" minOccurs="1" maxOccurs="1"/>
